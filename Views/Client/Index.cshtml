@model IList<faegtodo.Models.Client>;

@{
    ViewData["Title"] = "Clientes";
}

@Html.ActionLink("Novo", "Form", "Client", null, new {@class = "btn btn-primary"})

<h2>Clientes</h2>
<table class="table">
    <thead>
        <th>Id</th>
        <th>Nome</th>
        <th>Endere√ßo</th>
    </thead>
    <tbody>
        @foreach (var umclient in Model)
        {
        <tr>
            <td>@umclient.Id</td>
            <td>@umclient.Name</td>
            <td>@umclient.Address</td>
        </tr>
        }
    </tbody>
</table>